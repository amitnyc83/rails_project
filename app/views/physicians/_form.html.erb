<h2><%= form_for (@physician) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :specialty %>
  <%= f.text_field :specialty %></h2>
  <br>
  <%= f.submit %>
  <% end %>
  <br>
